{
  "name": "where-watch",
  "version": "1.0.2",
  "bin": {
    "where-watch": "index.js"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && javascript-obfuscator docs && node obfuscate.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "browser": "start http://localhost:3000",
    "docs": "serve docs",
    "run-bin": "run-p browser docs",
    "android-init": "cap init --web-dir docs && cap add android && capacitor-assets generate --android",
    "android": "npm run build && cap sync && cap open android",
    "full": "npm run build && npm run run-bin"
  },
  "dependencies": {
    "@angular/animations": "^14.3.0",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@ngxs/storage-plugin": "^3.8.1",
    "@ngxs/store": "^3.8.1",
    "npm-run-all2": "^6.0.6",
    "rxjs": "~7.5.0",
    "serve": "^14.2.0",
    "tslib": "^2.3.0",
    "zod": "^3.21.4",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.12",
    "@angular/cli": "^14.2.12",
    "@angular/compiler-cli": "^14.3.0",
    "@capacitor/android": "^5.2.2",
    "@capacitor/assets": "^2.0.4",
    "@capacitor/cli": "^5.2.2",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~4.0.0",
    "javascript-obfuscator": "^4.0.2",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "tailwindcss": "^3.3.3",
    "typescript": "~4.6.2"
  }
}
